{
  "name": "burbarshop",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "npm run test && node scripts/setup-env.cjs && prisma generate && next build",
    "postinstall": "node scripts/setup-env.cjs && prisma generate",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "check": "./scripts/check-code.sh",
    "type-check": "tsc --noEmit --skipLibCheck",
    "db:seed": "tsx prisma/seed.ts",
    "db:setup": "node scripts/setup-database.js",
    "db:reset": "node scripts/setup-env.cjs && prisma migrate reset",
    "db:sample": "node scripts/setup-database.js --reset --seed-sample",
    "db:realistic": "node scripts/realistic-data-generator.js",
    "db:fresh": "npm run db:reset && npm run db:realistic",
    "extract:etsy": "node scripts/etsy-extractor.js",
    "extract:etsy-api": "node scripts/etsy-api-extractor.js",
    "import:etsy": "node scripts/import-etsy-listings.js",
    "images:download": "node scripts/download-and-optimize-images.js --download",
    "images:strategy": "node scripts/download-and-optimize-images.js --strategy-only",
    "input:manual": "node scripts/manual-data-input.js",
    "mcp:stripe": "node mcp-servers/stripe-server.js",
    "mcp:orders": "node mcp-servers/order-management-server.js",
    "mcp:database": "node mcp-servers/database-server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "pre-push": "npm run lint && npm run test",
    "validate": "node scripts/validate-code.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format:fix": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test:unit": "jest --testPathPattern='__tests__'",
    "test:integration": "jest --testPathPattern='integration'",
    "test:e2e": "playwright test",
    "test:api": "jest --testPathPattern='api'",
    "db:migrate": "node scripts/setup-env.js && prisma migrate dev",
    "db:deploy": "node scripts/setup-env.js && prisma migrate deploy"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.16.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@prisma/client": "^5.7.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-form": "^0.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sendgrid/mail": "^8.1.5",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.5.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@vercel/blob": "^1.1.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.0",
    "formidable": "^3.5.4",
    "lucide-react": "^0.525.0",
    "next": "^15.0.0",
    "next-auth": "^4.24.0",
    "node-fetch": "^2.7.0",
    "puppeteer": "^24.14.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "stripe": "^14.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.0.1",
    "cheerio": "^1.1.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.7.0",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0"
  }
}
